{
  "name": "@bantai-dev/shared",
  "version": "1.0.0",
  "description": "Shared utilities for @bantai-dev",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bosquejun/bantai.git",
    "directory": "packages/shared"
  },
  "homepage": "https://bantai.vercel.app/",
  "bugs": {
    "url": "https://github.com/bosquejun/bantai/issues"
  },
  "keywords": [
    "shared",
    "shared-utilities",
    "bantai"
  ],
  "author": {
    "name": "Jun",
    "email": "bosquejun@gmail.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@bantai-dev/typescript-config": "workspace:*",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.1",
    "typescript": "5.9.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "nanoid": "^5.1.6"
